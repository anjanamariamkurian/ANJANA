  #include<stdio.h>
  #include<iostream.h>
  int main()
  {
	int a[3][3],b[3][3],sum[3][3],i,j;
	cout<<"enter the 1st matrix";
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			cin>>a[i][j];
		}
	}
	cout<<"enter the 2nd matrix";
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			cin>>b[i][j];
		}
	}
	cout<<"first matrix:\n";

	for(i=0;i<3;i++)
	{
		cout<<"\n";
		for(j=0;j<3;j++)
		{
			cout<<a[i][j]<<"\t";
		}
	}
	cout<<"second matrix:\n";

	for(i=0;i<3;i++)
	{
		cout<<"\n";
		for(j=0;j<3;j++)
		{
			cout<<b[i][j]<<"\t";
		}
	}

	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			sum[i][j]=a[i][j]+b[i][j];
		}
	}
	for(i=0;i<3;i++)
	{
		cout<<"\n";
		for(j=0;j<3;j++)
		{
			cout<<sum[i][j]<<"\t";
		}
	}
  return 0;
  }
