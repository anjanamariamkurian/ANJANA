  #include<iostream.h>
  #include<stdio.h>
  int main()
  {
  int a[20],size,i,big;
  cout<<"enter size of array";
  cin>>size;
  cout<<"enter elements of array";
  for(i=0;i<size;i++)
	cin>>a[i];
  big=a[0];
  for(i=1;i<size;i++)
  {
	if(big<a[i])
		big=a[i];

  }
  cout<<"biggest element:"<<big;
  return 0;
  }
