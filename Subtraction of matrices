  #include<stdio.h>
  #include<iostream.h>
  int main()
  {
	int a[3][3],b[3][3],sub[3][3],i,j;
	cout<<"enter 1st matrix";
	for(i=0;i<3;i++)
		for(j=0;j<3;j++)
			cin>>a[i][j];
	cout<<"enter 2nd matrix";
	for(i=0;i<3;i++)
		for(j=0;j<3;j++)
			cin>>b[i][j];
	cout<<"first matrix:";
	for(i=0;i<3;i++)
	{
		cout<<"\n";
		for(j=0;j<3;j++)
			cout<<a[i][j];
	}
	cout<<"second matrix:";
	for(i=0;i<3;i++)
	{
		cout<<"\n";
		for(j=0;j<3;j++)
			cout<<b[i][j];
	}
	for(i=0;i<3;i++)
		for(j=0;j<3;j++)
			sub[i][j]=a[i][j]-b[i][j];
		for(i=0;i<3;i++)
	{
		cout<<"\n";
		for(j=0;j<3;j++)
			cout<<sub[i][j];
	}
	  return 0;
	  }
