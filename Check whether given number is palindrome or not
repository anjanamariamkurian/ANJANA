   #include<iostream.h>
   #include<stdio.h>
   int main()
   {
	int num,temp,sum=0,rem;
	cout<<"enter any number";
	cin>>num;
	temp=num;
	while(num>0)
	{
		rem=num%10;
		sum=(sum*10)+rem;
		num=num/10;
	}
	if(temp==sum)
	{
		cout<<"palindrome number";
	}
	else
	{
		cout<<"not a palindrome number";
	}
	return 0;
   }
